---
title: {{ module.modulename if module else "API Reference" }}
layout: pdoc
---
{% block body %}
    <nav class="pdoc">
        <label id="navtoggle" for="togglestate" class="pdoc-button">{% include 'resources/navtoggle.svg' %}</label>
        <input id="togglestate" type="checkbox" aria-hidden="true" tabindex="-1">
        <div>{% block nav %}{% endblock %}</div>
    </nav>
    {% block content %}{% endblock %}
{% endblock body %}

{% filter minify_css %}
    {% block style %}
        <style>{% include "syntax-highlighting.css" %}</style>
        <style>{% include "theme.css" %}</style>
        <style>{% include "layout.css" %}</style>
        <style>{% include "content.css" %}</style>
        <style>{% include "custom.css" %}</style>
    {% endblock %}
{% endfilter %}